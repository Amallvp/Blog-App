{
    "version": 2,
    "builds": [
        {
          "src": "package.json",
          "use": "@vercel/node",
          "config": {
            "distDir": "build"
          }
        }
      ],
    "routes": [
      {
            "src": "/api/(.*)",
      "dest": "/api/index.js"
      }
      
    ]
  }